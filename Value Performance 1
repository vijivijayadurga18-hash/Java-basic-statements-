import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
      Scanner AB=new Scanner(System.in);
      int a=AB.nextInt();
      float b=AB.nextFloat();
      System.out.println(a);
      System.out.printf("%.2f\n",b);
    }
}
