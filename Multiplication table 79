import java.util.*;

class Solution{

    public static void main(String[]args){

        Scanner sc=new Scanner(System.in);

        int n=sc.nextInt();

        if(n<1||n>9){

            System.out.println("Invalid Input");

        }else{

            for(int i=1;i<=n;i++){

              System.out.println(n+" x "+i+" = "+(n*i));  

            }

        }

    }

}
