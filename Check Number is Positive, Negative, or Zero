import java.util.Scanner;
public class main{
    public static void main(String[]args){
        Scanner sc = new Scanner (System.in);
        int num = sc.nextInt();
        
        if(num>0){
             System.out.print("positive");
        }else if(num<0){
          System.out.print("negative");
        }else {
            System.out.print("zero");
        }
    }
}
